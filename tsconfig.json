{
  "compilerOptions": {
    "target": "ES2018",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "pages/_app.jsx",
    "components/navBar/desktop/NavBar_D_DropdownSuggestionComponent.js"
  ],
  "exclude": [
    "node_modules"
  ],
  "types": [
    "vitest/importMeta"
  ]
}